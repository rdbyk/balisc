S = [];
S($+1,:) = [ 1 2 ];
assert_checkequal(size(S),[1 2]);
assert_checkequal(S(1,:),[1 2]);
S = [];
S($+1,:) = [ "a" "b" ];
assert_checkequal(size(S),[1 2]);
assert_checkequal(S(1,:),["a" "b"]);
S = [];
S($+1,:) = [ %t %f ];
assert_checkequal(size(S),[1 2]);
assert_checkequal(S(1,:),[%t %f]);
S = [];
S($+1,:) = [ %z %z];
assert_checkequal(size(S),[1 2]);
assert_checkequal(S(1,:),[%z %z]);
