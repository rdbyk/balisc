s=sparse([1 0 2 0 0 ; 0 -4 0 0 7 ]);
y = median(s);
res = sparse(median(full(s)));
assert_checkequal(y, res);
y = median(s, "r");
res = sparse(median(full(s), "r"));
assert_checkequal(y, res);
y = median(s, "c");
res = sparse(median(full(s), "c"));
assert_checkequal(y, res);
y = median(s, "m");
res = sparse(median(full(s), "m"));
assert_checkequal(y, res);
y = median(s, 1);
res = sparse(median(full(s), 1));
assert_checkequal(y, res);
