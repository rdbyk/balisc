r = correl(1:10, 1:10);
assert_checkequal(r, 1);
fre = eye(10, 10);
r = correl(1:10, 1:10, fre);
assert_checkequal(r, 1);
fre = ones(10, 10);
r = correl(1:10, 1:10, fre);
assert_checkequal(r, 0);
r = correl(1:10, -(1:10));
assert_checkequal(r, -1);
fre = eye(10, 10);
r = correl(1:10, -(1:10), fre);
assert_checkequal(r, -1);
fre = ones(10, 10);
r = correl(1:10, -(1:10), fre);
assert_checkequal(r, 0);
