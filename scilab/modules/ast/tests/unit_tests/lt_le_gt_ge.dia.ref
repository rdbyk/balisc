r = 2;
R = [1,2;3,4];
R3(:,:,1) = R;
R3(:,:,2) = R';
assert_checkfalse(r < r);
assert_checkfalse(r > r);
assert_checktrue(r >= r);
assert_checktrue(r <= r);
assert_checkfalse(R < R);
assert_checkfalse(R > R);
assert_checktrue(R >= R);
assert_checktrue(R <= R);
assert_checkfalse(R3 < R3);
assert_checkfalse(R3 > R3);
assert_checktrue(R3 <= R3);
assert_checktrue(R3 >= R3);
