A = [1 2; 3 4];
ierr = execstr("A''(1, 2)", "errcatch");
assert_checktrue(ierr == 0);
assert_checkequal(A'(1, 2), A(2, 1));
assert_checkequal(A'(2, 1), A(1, 2));
A(2, 1) = %i;
ierr = execstr("A.''(1, 2)", "errcatch");
assert_checktrue(ierr == 0);
assert_checkequal(A.'(1, 2), A(2, 1));
assert_checkequal(A.'(2, 1), A(1, 2));
