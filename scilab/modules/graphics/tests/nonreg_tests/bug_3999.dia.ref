x = 1:10;
plot(x);
clf();
fig = gcf();
assert_checkequal(size(fig.children), [1,1]);
assert_checkequal(fig.children(1).children, []);
plot(x);
plot3d(x,x,cos(x)'*sin(x));
clf(0:3);
fig = findobj("figure_id",0);
assert_checkequal(size(fig.children), [1,1]);
assert_checkequal(fig.children(1).children, []);
