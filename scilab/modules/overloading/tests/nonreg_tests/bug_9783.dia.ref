function [q] = new(x)
  q = mlist(['X','x'], x);
endfunction
function [q2] = %X_i_X(I,q1,q2)
  if (I == []) 
    return; 
  else
    x2 = q2.x;
    x1 = q1.x; 
    x2(I) = x1;
    q2 = new(x2);
  end
endfunction
q0 = new([])
 q0  = 
       q0(1)
  "X"  "x"
       q0(2)
    []
q1 = new([91,93])
 q1  = 
       q1(1)
  "X"  "x"
       q1(2)
   91.   93.
q2 = new([10,20,30,40])
 q2  = 
       q2(1)
  "X"  "x"
       q2(2)
   10.   20.   30.   40.
q2([1,3]) = q1;
q2_ref = new([91 20 93 40]);
assert_checkequal(q2,q2_ref);
q2([]) = q0;
assert_checktrue(execstr("q2([])=q0","errcatch")==0);
