clear uicontrol_generic_test
exec("SCI/modules/gui/tests/unit_tests/uicontrol_generic_test.sci");
if uicontrol_generic_test("popupmenu") <> 0 then bugmes();quit;end
h = uicontrol("parent", scf(), "style", "popupmenu");
if get(h, "relief") <> "flat" then bugmes();quit;end
if ~isempty(get(h, "value")) then bugmes();quit;end
set(h, "string", "Item1|Item2");
set(h, "value", 1);
if get(h, "value") <> 1 then bugmes();quit;end
set(h, "string", "test|for|multiline|string");
if or(get(h, "string") <> ["test","for","multiline","string"]) then bugmes();quit;end
set(h, "string", ["a" "row" "vector" "string" "property"]);
if or(get(h, "string") <> ["a" "row" "vector" "string" "property"]) then bugmes();quit;end
set(h, "string", ["a";"column";"vector";"string";"property"]);
if or(get(h, "string") <> ["a" "column" "vector" "string" "property"]) then bugmes();quit;end
